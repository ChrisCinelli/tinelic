{@view name="header_view"/}

<div class="container">
<div class="row">
<div class="col-xs-9">
<h3>Page Views</h3><hr>
	<div id="pageviews" style="width:100%; height:400px;"></div>
<h3>Page Time</h3><hr>
	<div id="pagetime" style="width:100%; height:400px;"></div>
</div>
<div class="col-xs-3">
