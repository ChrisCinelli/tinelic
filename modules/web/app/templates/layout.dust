<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="{_t_prefix}/css/style.css" rel="stylesheet">
<title>{title}</title>
<script src="{_t_prefix}/js/require.js"></script>
<script src="//code.jquery.com/jquery-2.1.3.js"></script>
<script>
requirejs.config({
    baseUrl: "{_t_prefix}/app",
    paths:{
		"tinybone":"../tinybone",
		"lodash":"../js/lodash",
		"dust":"../js/dust",
		"safe":"../js/safe"
	},
	shim: {
		'dust': {
			exports: 'dust'
		}
	}
})
define("backctx","/restapi/");
</script>
</head>
<body id="content">
{@view name=_t_main_view /}
<script>requirejs(["app"], function (App) { (new App()).init(_t_app_wire); })</script>
</body>
</html>
